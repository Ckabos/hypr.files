
{
    // PulseAudio
    "pulseaudio": {
        "scroll-step": 1,
        "format": "{icon} {volume}%",
        "format-muted": "",  // icono muted universal
        "format-icons": {
            "default": [
                "",  // volumen bajo
                "",  // volumen medio
                " "   // volumen alto
            ]
        }
    }
}

